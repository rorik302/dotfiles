[user]
	email = rorik302@gmail.com
	name = Aleksandr Danilenko
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	side-by-side = true
	line-numbers = true
[merge]
	conflictStyle = zdiff3
[init]
	defaultBranch = main
[fetch]
	prune = true
[credential]
	helper = /usr/lib/git-core/git-credential-libsecret
